<div class="page-header">
  <h1>Listing all categories</h1>
</div>
<% @category.each do |category| %>
<div class="row">
  <div class="col-md-6">
    <div class="well">
      <div class="caption">
        <h3><%= link_to "#{category.name}", category_path(category) %></h3>
      </div>
    </div>
  </div>
</div>
<% end %>
<%= render 'layouts/paginate', obj: @category %>